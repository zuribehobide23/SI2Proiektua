<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">
	<managed-bean>
		<managed-bean-name>mainBeanHibernate</managed-bean-name>
		<managed-bean-class>eredua.bean.MainBeanHibernate</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>loginBeanHibernate</managed-bean-name>
		<managed-bean-class>eredua.bean.LoginBeanHibernate</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>registerBeanHibernate</managed-bean-name>
		<managed-bean-class>eredua.bean.RegisterBeanHibernate</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>createRideBeanHibernate</managed-bean-name>
		<managed-bean-class>eredua.bean.CreateRideBeanHibernate</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>databidaiakBeanHibernate</managed-bean-name>
		<managed-bean-class>eredua.bean.DataBidaiakBeanHibernate</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>finddataBeanHibernate</managed-bean-name>
		<managed-bean-class>eredua.bean.FindDataBeanHibernate</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>Main.xhtml</display-name>
		<from-view-id>/Main.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Login</from-outcome>
			<to-view-id>/Login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Main.xhtml</display-name>
		<from-view-id>/Main.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>QueryRides</from-outcome>
			<to-view-id>/Register.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Main.xhtml</display-name>
		<from-view-id>/Main.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Bidaiak</from-outcome>
			<to-view-id>/Bidaiak.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Bidaiak.xhtml</display-name>
		<from-view-id>/Bidaiak.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>FindData</from-outcome>
			<to-view-id>/FindData.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Login.xhtml</display-name>
		<from-view-id>/Login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Traveler</from-outcome>
			<to-view-id>/Traveler.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>Driver</from-outcome>
			<to-view-id>/Driver.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Driver.xhtml</display-name>
		<from-view-id>/Driver.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>CreateRide.xhtml</from-outcome>
			<to-view-id>/CreateRide.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Traveler.xhtml</display-name>
		<from-view-id>/Traveler.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>QueryRides.xhtml</from-outcome>
			<to-view-id>/QueryRides.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
